{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/reverso", "dest": "/index.js" },
    {
      "src": "/reverso/:word",
      "dest": "/index.js",
      "methods": ["GET"]
    },
    { "src": "/reverso/:word/*", "dest": "/index.js", "methods": ["GET"] }
  ]
}
